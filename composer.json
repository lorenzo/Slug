{
    "name": "muffin/slug",
    "description": "Slugging support for CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "slug"
    ],
    "homepage": "https://github.com/usemuffin/slug",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/slug/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/slug/issues",
        "source": "https://github.com/usemuffin/slug"
    },
    "require": {
        "cakephp/cakephp": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "cocur/slugify": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Slug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Slug\\Test\\": "tests"
        }
    }
}
